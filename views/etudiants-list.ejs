<!DOCTYPE html>
<html lang="en">
<head>
	<% include partials/head %>
</head>
<body class="container">

	<header>
		<% include partials/header %>
	</header>
	<nav>
		<div class="nav-wrapper">
			<div class="col s12">
				<a href="/" class="breadcrumb">Acceuil</a>
				<a href="#!" class="breadcrumb">Gestions des Ã©tudiants</a>
			</div>
		</div>
	</nav>
	<section>
		<div class="container">
			<div class="section">
				<h2> Etudiants</h2>
				<hr>
				<br>
				<div class="row">

					<!-- programme -->
					<% etudiants.forEach(function(etudiant){ %>
					<div class="col s12 m6">
						<div class="card">
							<div class="card-image">

								
							</div>
							<div class="card-content">
								<p>
									<i class="large material-icons">account_circle</i>
									<%= etudiant.nom %> -- <%= etudiant.prenom %>
								</p>
								<hr>
								<center>
									<p>
										<a class="waves-effect waves-light" href="/programmes-list/<%= etudiant._id %>">Programmes</a>
										<br>
										<a class="waves-effect waves-light">Cours</a>
										<br>
										<a class="waves-effect waves-light">Notes</a>

									</p>
								</center>
							</div>
						</div>
					</div>
					<% }); %> 
				</div>


			</div>
		</div>
	</section>
	<footer class="footer-copyright">
		<% include partials/footer %>
	</footer>
</body>
</html>
