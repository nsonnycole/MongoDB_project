<!DOCTYPE html>
<html lang="en">
<head>
	<% include partials/head %>
</head>
<body class="container">

	<header>
		<% include partials/header %>
	</header>

	<section>
		<div class="container">
			<div class="section">
				<h2> Etudiants</h2>
				<hr>
				<br>
				<div class="row">

					<!-- programme -->
					<% etudiants.forEach(function(etudiant){ %>
					<div class="col s12 m6">
						<div class="card">
							<div class="card-image">

								<a class="btn-floating halfway-fab waves-effect waves-light red" href="/programmes-list/<%=etudiant._id %>" ><i class="material-icons">add</i></a>
							</div>
							<div class="card-content">
								<%= etudiant.nom %> -- <%= etudiant.prenom %>

								<p>
									<a class="waves-effect waves-light btn-small" href="/programmes-list/<%= etudiant._id %>">Programmes</a>
									<br>
									<a class="waves-effect waves-light btn-small">Cours</a>
									<br>
									<a class="waves-effect waves-light btn-small">Notes</a>

								</p>
							</div>
						</div>
					</div>
					<% }); %> 
				</div>


			</div>
		</div>
	</section>
	<footer class="footer-copyright">
		<% include partials/footer %>
	</footer>
</body>
</html>
