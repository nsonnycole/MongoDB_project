<!DOCTYPE html>
<html lang="en">
<head>
	<% include partials/head %>
</head>
<body class="container">

	<header>
		<% include partials/header %>
	</header>
	<nav>
		<div class="nav-wrapper">
			<div class="col s12">
				<a href="/" class="breadcrumb">Acceuil</a>
				<a href="/profs-list" class="breadcrumb">Gestions des profs</a>
				<a href="!#" class="breadcrumb">Programmes</a>
			</div>
		</div>
	</nav>
	<section>
		<h2> Cours </h2>
		<hr>

		<br>
		<!-- Professeur -->
		<div class="row">

			<!-- programme -->
			<% notes.forEach(function(note){ %>
			<div class="col s12 m6">
				<div class="card">
					<div class="card-image">

					</div>
					<div class="card-content">

						<ul class="collapsible">
							<li>
								<div class="collapsible-header">Notes</div>
								<div class="collapsible-body">

									

									<% if (note.id_etudiant) { %>
									<ul>
										
										<li>
											<% note.id_etudiant.forEach(function(etudiant){ %>
											<%= etudiant.nom %><%= etudiant.prenom %>
											<% }); %> 

											<% note.id_cours.forEach(function(cours){ %>
											<%= cours.nom %>
											<% }); %> 

											
										</li>
									</ul>

									<%= note.note %>

									<% } %>
								</div>
							</li>
						</ul>

						
					</div>

					<!-- 				<a class="btn-floating halfway-fab waves-effect waves-light red" href="/cours-list/" ><i class="material-icons">add</i></a> -->

				</div>
			</div>
			<% }); %> 
		</div>

		<footer class="footer-copyright">
			<% include partials/footer %>
		</footer>

	</section>
</body>
</html>
